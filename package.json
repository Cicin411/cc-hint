{
    "name": "cc-hint",
    "displayName": "cc-hint",
    "description": "all hint in one for vue/react/others...",
    "version": "0.4.0",
    "engines": {
        "vscode": ">=1.75.0"
    },
    "categories": [
        "Programming Languages", "Snippets"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Cicin411/cc-hint",
        "directory": ""
    },
    "publisher": "cicin",
    "activationEvents": [
        "onLanguage:html",
        "onLanguage:vue",
        "onLanguage:tsx",
        "onLanguage:jsx"
    ],
    "main": "./dist/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "cc-hint.helloWorld",
                "title": "Hello World"
            }
        ],
        "configuration": [
            {
                "title": "cc-hint",
                "properties": {
                    "cc-hint.jsonsetting": {
                        "type": "array",
                        "default": [
                            {
                                "name": "quasar",
                                "jsonpath": ""
                            },
                            {
                                "name": "devui",
                                "jsonpath": ""
                            },
                            {
                                "name": "antdv",
                                "jsonpath": ""
                            },
                            {
                                "name": "elementplus",
                                "jsonpath": ""
                            }
                        ]
                    },
                    "cc-hint.labelsetting": {
                        "type": "object",
                        "default": {
                            "label_suffix": "  üêá",
                            "label_summary": "üëª"
                        }
                    }
                }
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "pnpm build",
        "build": "tsup",
        "dev": "tsup",
        "dev2": "tsup --watch src",
        "package": "vsce package --no-dependencies",
        "publish": "vsce publish --no-dependencies",
        "tscwatch": "tsc -watch -p ./",
        "tscbuild": "tsc -p ./"
    },
    "devDependencies": {
        "@types/node": "~16.18.12",
        "@types/vscode": "^1.75.0",
        "@vscode/test-electron": "^2.2.3",
        "esno": "^0.16.3",
        "tsup": "^6.5.0",
        "typescript": "^4.9.5"
    },
    "dependencies": {
        "pretty": "^2.0.0"
    }
}
